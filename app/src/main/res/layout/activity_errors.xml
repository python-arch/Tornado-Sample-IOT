<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="1dp"
    android:background="@color/white"
    tools:context=".ErrorsActivity">

    <LinearLayout
        android:padding="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background_main"
        android:orientation="vertical">

        <TextView
            android:id="@+id/Short_circuit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:text="Short circuit in outdoor temperature sensors (1-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/Compressor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:text="Compressor / ambient High Temperature (2-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/outdoor_unit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:text="Outdoor unit stop (3-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/open_circuit"
            android:layout_width="wrap_content"
            android:textColor="#000000"
            android:layout_height="wrap_content"
            android:text="Open circuit in outdoor temperature sensors (5-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/Increase_in_outdoor_unit_DC_current_6_0"
            android:layout_width="wrap_content"
            android:textColor="#000000"
            android:layout_height="wrap_content"
            android:text="Increase in outdoor unit DC current (6-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/Increase_in_outdoor_unit_DC_current_7_0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:text="Increase in outdoor unit AC current (7-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/cycle_temp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:text="Cycle temperature error (9-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/gas_leak"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:text="Gas leakage (9-4): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/error_9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:text="Outdoor PCB Corrupted (10-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/outdoor_fan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:text="Outdoor fan motor (11-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/outdoor_pcb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:text="Outdoor PCB fuse (12-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/speed_error"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:text="Compressor speed error (13-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/volt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:text="Voltage problem (14-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/miss_connection"
            android:layout_width="wrap_content"
            android:textColor="#000000"
            android:layout_height="wrap_content"

            android:text="Miss connection between indoor and outdoor (17-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/bad_coonnection"
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bad connection between indoor and outdoor (18-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/indoor_fan"
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Indoor fan motor (19-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/wrong_eeprom"
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Wrong EEPROM (20-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/wifi_error"
            android:layout_width="wrap_content"
            android:textColor="#000000"
            android:layout_height="wrap_content"
            android:text="Wifi error (24-0): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/room_temp_error_26_1"
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Room temperature error (26-1): 0"
            android:textSize="14sp"
            android:padding="8dp"/>

        <TextView
            android:id="@+id/room_temp_error_26_2"
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Evaporator temperature error (26-2): 0"
            android:textSize="13sp"
            android:padding="8dp"/>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>